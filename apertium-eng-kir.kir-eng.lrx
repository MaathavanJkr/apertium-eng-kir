<?xml version="1.0" encoding="UTF-8"?>
<rules>
	<rule>
		<match tags="num.ord"/>
		<match lemma="жол" tags="n.*">
			<select lemma="time" tags="n.*"/>
		</match>
	</rule>
</rules>

